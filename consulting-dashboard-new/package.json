{
  "name": "consulting-dashboard-new",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "db:push": "prisma db push",
    "db:seed": "tsx prisma/seed.ts",
    "db:studio": "prisma studio",
    "db:generate": "npm run db:generate:main && npm run db:generate:core && npm run db:generate:project && npm run db:generate:resource && npm run db:generate:timesheet && npm run db:generate:notification",
    "db:generate:main": "prisma generate",
    "db:generate:core": "prisma generate --schema=./prisma/core-service/schema.prisma",
    "db:generate:project": "prisma generate --schema=./prisma/project-service/schema.prisma",
    "db:generate:resource": "prisma generate --schema=./prisma/resource-service/schema.prisma",
    "db:generate:timesheet": "prisma generate --schema=./prisma/timesheet-service/schema.prisma",
    "db:generate:notification": "prisma generate --schema=./prisma/notification-service/schema.prisma",
    "db:push:all": "npm run db:push && npm run db:push:core && npm run db:push:project && npm run db:push:resource && npm run db:push:timesheet && npm run db:push:notification",
    "db:push:core": "prisma db push --schema=./prisma/core-service/schema.prisma",
    "db:push:project": "prisma db push --schema=./prisma/project-service/schema.prisma",
    "db:push:resource": "prisma db push --schema=./prisma/resource-service/schema.prisma",
    "db:push:timesheet": "prisma db push --schema=./prisma/timesheet-service/schema.prisma",
    "db:push:notification": "prisma db push --schema=./prisma/notification-service/schema.prisma",
    "db:studio:core": "prisma studio --schema=./prisma/core-service/schema.prisma --port 5556",
    "db:studio:project": "prisma studio --schema=./prisma/project-service/schema.prisma --port 5557",
    "db:studio:resource": "prisma studio --schema=./prisma/resource-service/schema.prisma --port 5558",
    "db:studio:timesheet": "prisma studio --schema=./prisma/timesheet-service/schema.prisma --port 5559",
    "db:studio:notification": "prisma studio --schema=./prisma/notification-service/schema.prisma --port 5560",
    "db:migrate:all": "npm run db:migrate && npm run db:migrate:timesheet && npm run db:migrate:notification",
    "db:migrate": "prisma migrate dev",
    "db:migrate:timesheet": "prisma migrate dev --schema=./prisma/timesheet-service/schema.prisma",
    "db:migrate:notification": "prisma migrate dev --schema=./prisma/notification-service/schema.prisma"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@hookform/resolvers": "^5.2.1",
    "@prisma/client": "^6.15.0",
    "@radix-ui/react-alert-dialog": "^1.1.15",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-toast": "^1.2.15",
    "bcryptjs": "^3.0.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cookie": "^1.0.2",
    "date-fns": "^4.1.0",
    "jose": "^6.1.0",
    "lucide-react": "^0.542.0",
    "next": "15.5.2",
    "prisma": "^6.15.0",
    "react": "19.1.0",
    "react-day-picker": "^9.9.0",
    "react-dom": "19.1.0",
    "react-hook-form": "^7.62.0",
    "recharts": "^3.2.0",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.3.1",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^4.1.8"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@playwright/test": "^1.55.0",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/bcryptjs": "^2.4.6",
    "@types/cookie": "^0.6.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "autoprefixer": "^10.4.21",
    "eslint": "^9",
    "eslint-config-next": "15.5.2",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-mock-extended": "^4.0.0",
    "postcss": "^8.5.6",
    "sqlite3": "^5.1.7",
    "tailwindcss": "^3.4.1",
    "ts-jest": "^29.4.1",
    "tsx": "^4.20.5",
    "typescript": "^5"
  }
}
