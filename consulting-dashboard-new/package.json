    "db:seed:all": "tsx prisma/seed-all.ts",
    "db:reset": "tsx prisma/reset-and-seed.ts",
    "db:seed:auth": "tsx prisma/seeds/core-seed.ts",
    "db:seed:reset": "tsx prisma/seed-with-reset.ts",
    "db:clear": "tsx prisma/seeds/utils/clear-data.ts",
    "db:generate": "npm run db:generate:auth && npm run db:generate:project && npm run db:generate:resource && npm run db:generate:timesheet && npm run db:generate:notification && npm run db:generate:finance && npm run db:generate:knowledge && npm run db:generate:parasol",
    "db:generate:project": "prisma generate --schema=./prisma/project-service/schema.prisma",
    "db:generate:resource": "prisma generate --schema=./prisma/resource-service/schema.prisma",
    "db:generate:timesheet": "prisma generate --schema=./prisma/timesheet-service/schema.prisma",
    "db:generate:notification": "prisma generate --schema=./prisma/notification-service/schema.prisma",
    "db:generate:auth": "prisma generate --schema=./prisma/auth-service/schema.prisma",
    "db:generate:finance": "prisma generate --schema=./prisma/finance-service/schema.prisma",
    "db:generate:knowledge": "prisma generate --schema=./prisma/knowledge-service/schema.prisma",
    "db:generate:parasol": "prisma generate --schema=./prisma/parasol-service/schema.prisma",
    "db:push:all": "npm run db:push:auth && npm run db:push:project && npm run db:push:resource && npm run db:push:timesheet && npm run db:push:notification && npm run db:push:finance && npm run db:push:knowledge && npm run db:push:parasol",
    "db:push:auth": "prisma db push --schema=./prisma/auth-service/schema.prisma",
    "db:push:project": "prisma db push --schema=./prisma/project-service/schema.prisma",
    "db:push:resource": "prisma db push --schema=./prisma/resource-service/schema.prisma",
    "db:push:timesheet": "prisma db push --schema=./prisma/timesheet-service/schema.prisma",
    "db:push:notification": "prisma db push --schema=./prisma/notification-service/schema.prisma",
    "db:push:finance": "prisma db push --schema=./prisma/finance-service/schema.prisma",
    "db:push:knowledge": "prisma db push --schema=./prisma/knowledge-service/schema.prisma",
    "db:push:parasol": "prisma db push --schema=./prisma/parasol-service/schema.prisma",
    "db:studio:project": "prisma studio --schema=./prisma/project-service/schema.prisma --port 5557",
    "db:studio:resource": "prisma studio --schema=./prisma/resource-service/schema.prisma --port 5558",
    "db:studio:timesheet": "prisma studio --schema=./prisma/timesheet-service/schema.prisma --port 5559",
    "db:studio:notification": "prisma studio --schema=./prisma/notification-service/schema.prisma --port 5560",
    "db:studio:auth": "prisma studio --schema=./prisma/auth-service/schema.prisma --port 5555",
    "db:studio:parasol": "prisma studio --schema=./prisma/parasol-service/schema.prisma --port 5561",
    "db:migrate:all": "npm run db:migrate && npm run db:migrate:timesheet && npm run db:migrate:notification",
    "db:migrate": "prisma migrate dev",
    "db:migrate:timesheet": "prisma migrate dev --schema=./prisma/timesheet-service/schema.prisma",
    "db:migrate:notification": "prisma migrate dev --schema=./prisma/notification-service/schema.prisma",
    "e2e": "playwright test",
    "e2e:ui": "playwright test --ui",
    "e2e:headed": "playwright test --headed",
    "e2e:debug": "playwright test --debug",
    "playwright:install": "playwright install",
    "playwright:mcp": "npx playwright-mcp"
  },
